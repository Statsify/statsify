{
	"$schema": "https://turborepo.org/schema.json",
	"globalDependencies": ["pnpm.lock", "tsconfig.json", ".swcrc", ".eslintrc"],
	"ui": "tui",
	"tasks": {
		"build": {
			"outputs": ["dist/**", ".next/**", "pkg/**"],
			"dependsOn": ["^build"]
		},
		"test:types": {
			"outputs": [],
			"dependsOn": ["^test:types"]
		},
		"test": {
			"outputs": [],
			"dependsOn": ["build", "^test"]
		},
		"test:coverage": {
			"outputs": ["coverage"],
			"dependsOn": ["build", "^test:coverage"]
		},
		"lint": {
			"outputs": [],
			"dependsOn": ["^lint"]
		}
	}
}
