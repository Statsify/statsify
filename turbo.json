{
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "build:watch": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build:watch"]
    },
    "test:types": {
      "outputs": [],
      "dependsOn": ["^test:types"]
    },
    "test": {
      "outputs": [],
      "dependsOn": ["^test"]
    },
    "lint": {
      "outputs": [],
      "dependsOn": ["^lint"]
    }
  }
}
