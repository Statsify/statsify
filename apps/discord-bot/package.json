{
  "name": "discord-bot",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "start": "node -r dotenv/config --enable-source-maps . dotenv_config_path=../../.env",
    "build": "swc src  --config-file ../../.swcrc --out-dir dist",
    "test:types": "tsc --noEmit",
    "lint": "TIMING=1 eslint './{src,tests}/**/*.{ts,tsx,js,jsx}' --fix"
  },
  "dependencies": {
    "@statsify/api-client": "workspace:^",
    "@statsify/assets": "workspace:^",
    "@statsify/discord": "workspace:^",
    "@statsify/rendering": "workspace:^",
    "@statsify/schemas": "workspace:^",
    "@statsify/util": "workspace:^",
    "@typegoose/typegoose": "^9.8.1",
    "axios": "^0.27.2",
    "discord-api-types": "^0.33.1",
    "dotenv": "^16.0.1",
    "i18next": "^21.8.5",
    "i18next-fs-backend": "^1.1.4",
    "mongoose": "^6.3.4",
    "reflect-metadata": "^0.1.13",
    "short-uuid": "^4.2.0",
    "skia-canvas": "^0.9.29",
    "tiny-discord": "https://github.com/timotejroiko/tiny-discord.git#commit=63c423b55ac40fe1842532f51b0879e9e55f66ab",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/i18next-fs-backend": "^1.1.2"
  },
  "imports": {
    "#arguments": "./dist/arguments/index.js",
    "#components": "./dist/components/index.js",
    "#profiles/*": "./dist/profiles/*.js",
    "#services": "./dist/services/index.js",
    "#constants": "./dist/constants.js"
  }
