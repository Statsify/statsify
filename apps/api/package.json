    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.1.1",
    "@nestjs/core": "^10.1.1",
    "@nestjs/platform-fastify": "^10.1.1",
    "@nestjs/swagger": "^7.1.2",
    "@sentry/node": "^7.60.0",
    "@sentry/tracing": "^7.60.0",
    "@statsify/api-client": "*",
    "@statsify/assets": "*",
    "@statsify/logger": "*",
    "@statsify/rendering": "*",
    "@statsify/schemas": "*",
    "@statsify/util": "*",
    "@swc/helpers": "^0.5.1",
    "@typegoose/typegoose": "^11.4.0",
