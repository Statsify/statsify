    "#auth": {
      "types": "./src/auth/index.ts",
      "default": "./dist/auth/index.js"
    },
    "#commands": {
      "types": "./src/commands/index.ts",
      "default": "./dist/commands/index.js"
    },
    "#dtos": {
      "types": "./src/dtos/index.ts",
      "default": "./dist/dtos/index.js"
