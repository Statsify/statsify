{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node --enable-source-maps .",
    "prod-start": "xvfb-run -s \"-ac -screen 0 600x800x24\" node --enable-source-maps .",
    "build": "swc src  --config-file ../../.swcrc --out-dir dist",
